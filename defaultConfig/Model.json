                      "acpiId": "4",
                      "timerInterval": 1000,
                      "transformFunc": {
                        "type": "function",
                        "value": "(value) => {;return  (Number.isNaN(value)?0:Math.trunc(value * 100)).toString() + \"%\" }"
                      }
